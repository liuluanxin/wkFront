
.content {
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
}
.logo {
	height: 200rpx;
	width: 200rpx;
	margin-top: 200rpx;
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 50rpx;
}
.text-area {
	display: flex;
	justify-content: center;
}
.item {
	box-sizing: border-box;
	height: 200rpx;
	width: 100%;
	margin-bottom: 10px;
	background: #ccc;
}
.title {
	font-size: 36rpx;
	color: #8f8f94;
}
.cgray {
	color: gray;
}
.basement {
	padding: 10rpx 20rpx;
	border-bottom: 1px solid #ccc;
}
.top_search {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
}
.top_search_nav {
	display: flex;
	align-items: center;
	width: 45%;
}
.result {
	margin-right: 20rpx;
}
.search_select {
	display: flex;
	flex: 1;
	justify-content: space-between;
	align-items: center;
}
.main {
	padding: 10rpx 20rpx;
}
.dataList {
	display: flex;
	justify-content: space-between;
	margin: 20rpx 10rpx;
}
.dataListRight {
	display: flex;
	justify-content: space-between;
	align-items: center;
	flex: 1;
	margin-right: 50rpx;
}
.dataListLeft {
	display: flex;
	justify-content: space-between;
	align-items: center;
	width: 200rpx;
}
.big_time {
	display: inline-block;
	color:#000000;
	margin: 10rpx 0;
	font-size: 32rpx;
}
.dataListTitle {
	font-size: 28rpx;
}
.dataListTime {
	font-size: 28rpx;
	color: #888;
}
.line {
	color: #bbb;
}

