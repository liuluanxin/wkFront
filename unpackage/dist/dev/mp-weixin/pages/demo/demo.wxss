
.center_title {
	width: 100%;
	display: inline-block;
	margin-bottom: 20rpx;
	text-align: center;
	font-size: 34rpx;
	background-color: #e6e6e6;
}
.list {
	position: relative;
	display: flex;
	justify-content: space-between;
	align-items: center;
	width: 100%;
	height: 55rpx;
	color: #fff;
	padding: 0 40rpx;
	margin: 10px 0;
	border-radius: 8px;
	box-sizing: border-box;
	box-shadow: 0 10px 8px #ccc;
	background-color: #0088ce;
}


#tab {
	width: 100%;
	display: flex;
}
.tabName {
	text-align: center;
	width: 33%;
	display: inline-block;
	height: 60rpx;
	line-height: 60rpx;
	white-space: nowrap;
}
.tabName_text {
	display: inline-block;
	width: 100%;
	height: 100%;
}
.active_text {
	background-color: #fddd9b;
	color: #ffffff;
}
#tabContent {
	position: absolute;
	width: 100%;
	height: 93%;
}

