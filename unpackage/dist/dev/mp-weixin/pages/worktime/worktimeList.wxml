<view class="imt-calendar"><view class="calendar-month-wrapper"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="calendar-icon calendar-icon-leftArrow" style="{{'color:'+(current&&color)+';'}}" bindtap="__e"></view><view class="calendar-month">{{currentTime}}</view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="calendar-icon calendar-icon-rightArrow" style="{{'color:'+(current==calendar.length-1||color)+';'}}" bindtap="__e"></view></view><view class="calendar-week-wrapper"><block wx:for="{{week}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="calendar-week">{{item}}</view></block></view><swiper class="calendar-date-swiper" current="{{current}}" data-event-opts="{{[['change',[['e2',['$event']]]]]}}" bindchange="__e"><block wx:for="{{calendar}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="calendar-date-wrapper"><block wx:for="{{item}}" wx:for-item="value" wx:for-index="key" wx:key="key"><view class="calendar-date" style="{{'background:'+(value.selected&&color)+';'+('color:'+(value.selected&&'#fff')+';')}}">{{value.date}}</view></block></swiper-item></block></swiper></view>