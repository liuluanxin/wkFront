
.center_title {
	width: 100%;
	display: inline-block;
	margin-bottom: 20rpx;
	text-align: center;
	font-size: 34rpx;
	background-color: #e6e6e6;
}
.list {
	position: relative;
	display: flex;
	justify-content: space-between;
	align-items: center;
	width: 100%;
	height: 55rpx;
	color: #fff;
	padding: 0 40rpx;
	margin: 10px 0;
	border-radius: 8px;
	box-sizing: border-box;
	box-shadow: 0 10px 8px #ccc;
	background-color: #0088ce;
}

